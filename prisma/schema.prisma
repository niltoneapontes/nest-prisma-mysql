generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model BubbleTeamMember {
  id String @id
  userId String?
  name String
  role String
  email String
  phone String
  birthdate String?
  pix String
  createdAt String
  updatedAt String
}

model Punch {
  id String @id
  memberId String
  memberName String
  type String
  datetime String
  createdAt String
  updatedAt String
}

model User {
  userId String @id
  username String
  password String
  createdAt String
  updatedAt String
}