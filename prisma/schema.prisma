generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model BubbleTeamMember {
  id String @id
  name String
  role String
  email String
  phone String
  birthdate String?
  pix String
}

model Punch {
  id String @id
  memberId String
  memberName String
  type String
  datetime String
}